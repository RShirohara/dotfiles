{
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features",
    "editor.suggest.insertMode": "replace"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features",
    "editor.suggest.insertMode": "replace"
  },
  "[json][jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace"
  },
  "[markdown]": {
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
    "editor.quickSuggestions": {
      "comments": "off",
      "other": "off",
      "strings": "off"
    },
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.wordWrap": "bounded",
    "files.trimTrailingWhitespace": false
  },
  "diffEditor.diffAlgorithm": "advanced",
  "diffEditor.renderSideBySide": false,
  "editor.dragAndDrop": false,
  "editor.fontFamily": "HackGen, 'Noto Sans CJK JP', monospace",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.showToolbar": "always",
  "editor.minimap.maxColumn": 100,
  "editor.quickSuggestions": {
    "other": "inline"
  },
  "editor.renderWhitespace": "all",
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "editor.suggest.localityBonus": true,
  "editor.suggest.matchOnWordStartOnly": false,
  "editor.suggest.showWords": false,
  "editor.suggestOnTriggerCharacters": false,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.tabSize": 2,
  "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?_",
  "editor.wordWrap": "bounded",
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "emmet.useInlineCompletions": true,
  "explorer.confirmDragAndDrop": false,
  "explorer.incrementalNaming": "smart",
  "explorer.sortOrder": "type",
  "extensions.ignoreRecommendations": true,
  "files.autoGuessEncoding": true,
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableCommitSigning": true,
  "git.mergeEditor": true,
  "git.openRepositoryInParentFolders": "never",
  "githubPullRequests.createOnPublishBranch": "never",
  "gitlens.graph.layout": "editor",
  "gitlens.graph.minimap.enabled": false,
  "javascript.inlayHints.enumMemberValues.enabled": true,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "literals",
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.inlayHints.variableTypes.enabled": true,
  "jest.autoRun": "on-save",
  "js/ts.implicitProjectConfig.checkJs": true,
  "js/ts.implicitProjectConfig.target": "ESNext",
  "markdown.preview.fontFamily": "'Noto Sans CJK JP', 'Noto Sans CJK', -apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', system-ui, 'Ubuntu', 'Droid Sans', sans-serif",
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "search.useGlobalIgnoreFiles": true,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.enableBell": true,
  "terminal.integrated.fontFamily": "'HackGen Console NF', monospace",
  "terminal.integrated.scrollback": 1000000,
  "terminal.integrated.smoothScrolling": true,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "literals",
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "update.mode": "none",
  "window.commandCenter": true,
  "workbench.colorTheme": "GitHub Dark",
  "workbench.editor.defaultBinaryEditor": "hexEditor.hexedit",
  "workbench.editor.languageDetection": false,
  "workbench.editor.scrollToSwitchTabs": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.preferredDarkColorTheme": "GitHub Dark",
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false,
  "zenMode.restore": false,
  "zenMode.silentNotifications": false
}
