{
  "[css]": {
    "cSpell.fixSpellingWithRenameProvider": false,
    "editor.defaultFormatter": "vscode.css-language-features",
    "editor.suggest.insertMode": "replace"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features",
    "editor.suggest.insertMode": "replace"
  },
  "[json][jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.quickSuggestions": {
      "strings": "inline"
    },
    "editor.suggest.insertMode": "replace"
  },
  "[markdown]": {
    "cSpell.advanced.feature.useReferenceProviderRemove": "/^#+\\s/",
    "cSpell.advanced.feature.useReferenceProviderWithRename": true,
    "cSpell.fixSpellingWithRenameProvider": true,
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
    "editor.quickSuggestions": {
      "comments": "off",
      "other": "off",
      "strings": "off"
    },
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.wordWrap": "bounded"
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "[yaml]": {
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "cSpell.spellCheckOnlyWorkspaceFiles": true,
  "diffEditor.experimental.showMoves": true,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.renderSideBySide": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.fontFamily": "HackGen, 'Noto Sans CJK JP', monospace",
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "editor.inlineSuggest.showToolbar": "always",
  "editor.minimap.maxColumn": 80,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.quickSuggestions": {
    "other": "inline"
  },
  "editor.renderWhitespace": "all",
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "editor.suggest.localityBonus": true,
  "editor.suggest.showWords": false,
  "editor.suggestOnTriggerCharacters": false,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.tabSize": 2,
  "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?_",
  "editor.wordWrap": "bounded",
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "emmet.useInlineCompletions": true,
  "explorer.incrementalNaming": "smart",
  "explorer.sortOrder": "type",
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.env": "properties",
    "*config": "properties",
    "*ignore": "properties"
  },
  "files.autoGuessEncoding": true,
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": "all",
  "git.confirmSync": false,
  "git.enableCommitSigning": true,
  "git.mergeEditor": true,
  "git.openRepositoryInParentFolders": "never",
  "git.pruneOnFetch": true,
  "gitlens.graph.layout": "editor",
  "gitlens.graph.minimap.enabled": false,
  "gitlens.rebaseEditor.ordering": "asc",
  "gitlens.views.commitDetails.files.layout": "tree",
  "javascript.inlayHints.enumMemberValues.enabled": true,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "literals",
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.inlayHints.variableTypes.enabled": true,
  "js/ts.implicitProjectConfig.checkJs": true,
  "markdown.preview.fontFamily": "'Noto Sans CJK JP', 'Noto Sans CJK', -apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', system-ui, 'Ubuntu', 'Droid Sans', sans-serif",
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "search.useGlobalIgnoreFiles": true,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.enableImages": true,
  "terminal.integrated.enableVisualBell": true,
  "terminal.integrated.fontFamily": "'HackGen Console NF', monospace",
  "terminal.integrated.scrollback": 1000000,
  "terminal.integrated.shellIntegration.enabled": false,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.wordSeparators": " ~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?_",
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "literals",
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "update.mode": "none",
  "workbench.colorTheme": "GitHub Dark Dimmed",
  "workbench.editor.defaultBinaryEditor": "hexEditor.hexedit",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "zenMode.hideLineNumbers": false,
  "zenMode.hideStatusBar": false,
  "zenMode.restore": false,
  "zenMode.silentNotifications": false
}
