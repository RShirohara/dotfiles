# Meta-plugin settings for "z-shell/@console-tools"

## cat -> bat
alias cat="bat --paging=never"

## ls -> exa
alias ls="exa --icons --group-directories-first --git"
alias l="ls -1a"
alias ll="ls -l"
alias lr="ll -R"
alias la="ll -a"
alias lt="ll -T"

## diff -> delta
alias diff="delta"

## grep -> ripgrep
alias grep="rg"

## find -> fd
alias find="fd"

## od -> hexyl
alias od="hexyl"
